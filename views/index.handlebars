{{#each article}}
<div class=col-md-12>
    <h1><a href="{{this.link}}" target="_blank">{{this.title}}</a> </h1>
    <p>{{this.description}}</p>
    <form action="/save" method="POST">
        <input name="_id" value="{{_id}}" type="hidden" />
        <button type="submit" class="btn btn-default">Save</button>
    </form>
</div>
{{/each}}